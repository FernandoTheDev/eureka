func int fib(n int) {
    if n <= 1 {
        return n
    }
    return fib(n - 1) + fib(n - 2)
}

func int sum_range(n int) {
    let total int = 0
    for i in 0..n {
        total = total + i
    }
    return total
}

// Chamadas múltiplas
print(fib(20))           // ~10,946 chamadas recursivas
print(sum_range(10000))  // 10,000 iterações
